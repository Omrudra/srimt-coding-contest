#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <stdbool.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    int i,j,n,p;
    bool f;
    scanf("%d",&n);
    for(i=1;i<=n;i++){
        scanf("%d",&p);
        f=false;
        for(j=0;j<=100;j++){
            if(p==pow(2,j))
                f=true;
        }
        if(f==false)
            printf("NO\n");
        else
            printf("YES\n");
        
        
    }
    return 0;
}
